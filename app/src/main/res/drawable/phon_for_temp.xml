<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid
                android:color="#ADBAB8"/>
            <corners
                android:radius="25dp"/>
            <gradient
                android:angle="90"
                android:startColor="#9C2E1943"
                android:endColor="#000000"
                android:centerColor="#290B45" />


            <stroke
                android:width="3dp"
                android:color="#000000"
                android:dashGap="0dp"
                android:dashWidth="10dp"/>

        </shape>
    </item>
</selector>
