<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid
                android:color="#FF0000"/>
            <corners
                android:radius="10dp"/>
            <size
                android:height="70dp"
                android:width="250dp"/>
            <stroke
                android:width="3dp"
                android:color="@android:color/black"
                android:dashGap="0dp"
                android:dashWidth="10dp"/>

        </shape>
    </item>
</selector>